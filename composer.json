{
  "name": "hive/benchmark",
  "description": "Hive Decoupled Benchmarking Library",
  "keywords": [
    "benchmark", "benchmark library", "benchmarking", "hive", "wax", "library", "helper"
  ],
  "homepage": "https://hive.github.io/benchmark/",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Jamie Peake",
      "email": "jamie.peake@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/hive/benchmark/issues",
    "wiki": "https://github.com/hive/benchmark/wiki",
    "source": "https://github.com/hive/benchmark",
    "examples": "https://github.com/hive/benchmark/examples"
  },
  "type": "Library",
  "require": {
    "php": ">5.5.0"
  },
  "autoload": {
    "psr-4": {
      "Hive\\Benchmark\\": ["source/", "source/Facade/"]
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  },
  "minimum-stability": "stable",
  "suggest": {
    "hive/profiler": "Allow profiling of the site with benchmark output"
  }
}
